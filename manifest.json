{
    "manifest_version": 2,
    "name": "Байрон",
    "description": "This extension demonstrates a browser action with kittens.",
    "version": "1.0",

    "permissions": [
	"http://ngs.ru/",
	"http://ngs24.ru/",
	"http://news.ngs.ru/",
	"http://news.ngs24.ru/",
	"http://www.ngs.ru/",
	"http://www.ngs24.ru/",
	"http://www.news.ngs.ru/",
	"http://www.news.ngs24.ru/"
    ],
	"icons": { "16": "byron-extension-16.png",
           "48": "byron-extension-48.png",
          "128": "byron-extension-128.png" },
    "browser_action": {"default_icon": "byron-extension-19.png"},

    "content_scripts": [
	{
	    "matches": [
		"http://ngs.ru/",
		"http://news.ngs.ru/*",
		"http://ngs24.ru/",
		"http://news.ngs24.ru/*",
		"http://www.ngs.ru/",
		"http://www.ngs24.ru/",
		"http://www.news.ngs.ru/",
		"http://www.news.ngs24.ru/"
	    ],
	    "js": ["jquery.js", "replacer.js"]
	}
    ],
    
    "web_accessible_resources": [
	"titles.txt"
    ]
}